# Cria uma pasta no seu PC
mkdir Data-Base
cd Data-Base

# Cria o README.md
echo "# Data-Base" >> README.md

# Inicia o git
git init
git add README.md
git commit -m "primeiro commit"

# Conecta com seu reposit√≥rio no GitHub
git branch -M main
git remote add origin https://github.com/IvsonMendonca/Data-Base.git
git push -u origin main
